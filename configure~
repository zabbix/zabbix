	if test "x$found_openssl" != "xOpenSSL" -a "x$found_openssl" != "xOpenSSL (PSK not supported)"; then
		as_fn_error $? "OpenSSL library libssl or libcrypto not found" "$LINENO" 5
	elif test "x$accept_openssl_version" != "xyes" -a "$mt_required" = "yes"; then
		as_fn_error $? "OpenSSL library version requirement for server or proxy not met (>= 1.1.0)" "$LINENO" 5
	elif test "x$accept_openssl_version" != "xyes"; then
		as_fn_error $? "OpenSSL library version requirement not met (>= 1.0.1)" "$LINENO" 5
