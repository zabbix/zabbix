		LIBS="$LIBS $LIBEVENT_LIBS"

		found_libevent="no"

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <event2/event.h>
#include <event2/thread.h>

int
main (void)
{

	struct event_base *evb;

	evb = event_base_new();

	evthread_use_pthreads();
	event_base_free(evb);

  ;
  return 0;
}
