else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


  test -n "$JAVAC" && break
done
test -n "$JAVAC" || JAVAC="no"

	if test "x$JAVAC" = "xno"; then
		as_fn_error $? "Unable to find \"javac\" executable in path" "$LINENO" 5
	fi
	for ac_prog in jar
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_JAR+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$JAR"; then
  ac_cv_prog_JAR="$JAR" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_JAR="$ac_prog"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
JAR=$ac_cv_prog_JAR
if test -n "$JAR"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $JAR" >&5
printf "%s\n" "$JAR" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


  test -n "$JAR" && break
done
test -n "$JAR" || JAR="no"

	if test "x$JAR" = "xno"; then
		as_fn_error $? "Unable to find \"jar\" executable in path" "$LINENO" 5
	fi
fi

found_ldap="no"


# Check whether --with-ldap was given.
if test ${with_ldap+y}
then :
  withval=$with_ldap;  if test "$withval" = "no"; then
            want_ldap="no"
            _libldap_with="no"
        elif test "$withval" = "yes"; then
            want_ldap="yes"
            _libldap_with="yes"
        else
            want_ldap="yes"
            _libldap_with=$withval
        fi

else $as_nop
  _libldap_with=no
fi


  if test "x$_libldap_with" != x"no"; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for LDAP support of ldap.h" >&5
printf %s "checking for LDAP support of ldap.h... " >&6; }

        if test "$_libldap_with" = "yes"; then
                if test -f /usr/local/openldap/include/ldap.h; then
                        LDAP_INCDIR=/usr/local/openldap/include/
                        LDAP_LIBDIR=/usr/local/openldap/lib/
                        found_ldap="yes"
                elif test -f /usr/include/ldap.h; then
                        LDAP_INCDIR=/usr/include
                        LDAP_LIBDIR=/usr/lib
                        found_ldap="yes"
                elif test -f /usr/local/include/ldap.h; then
                        LDAP_INCDIR=/usr/local/include
                        LDAP_LIBDIR=/usr/local/lib
                        found_ldap="yes"
                elif test -f /opt/homebrew/opt/openldap/include/ldap.h; then
                        LDAP_INCDIR="/opt/homebrew/opt/openldap/include"
                        LDAP_LIBDIR="/opt/homebrew/opt/openldap/lib"
                        found_ldap="yes"
                else
                        found_ldap="no"
                        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
                fi
        else
                if test -f $_libldap_with/include/ldap.h; then
                        LDAP_INCDIR=$_libldap_with/include
                        LDAP_LIBDIR=$_libldap_with/lib
                        _ldap_dir_lib="$LDAP_LIBDIR"

                        found_ldap="yes"
                else
                        found_ldap="no"
                        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
                fi
        fi

        if test "x$found_ldap" != "xno"; then

                { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
                LDAP_CPPFLAGS="-I$LDAP_INCDIR"
                LDAP_LDFLAGS="-L$LDAP_LIBDIR"

                ldap_ver=$(strings `find $LDAP_LIBDIR -name libldap.so` | grep OpenLDAP | grep -Eo '0-9+\.0-9+\.0-9+' | tr -d '.')
                if test -n "$ldap_ver" && test "$ldap_ver" -ge 261; then

printf "%s\n" "#define HAVE_LDAP_SOURCEIP 1" >>confdefs.h

                fi

                LDAP_LIBS="-lldap -llber $LDAP_LIBS"

                if test "x$enable_static" = "xyes"; then
                        LDAP_LIBS="$LDAP_LIBS -lgnutls -lpthread -lsasl2"
                elif test "x$enable_static_libs" = "xyes"; then
                        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking compatibility of static LDAP libs" >&5
printf %s "checking compatibility of static LDAP libs... " >&6; }
                        test "x$static_linking_support" = "xno" -a -z "$_ldap_dir_lib" && as_fn_error $? "\"Compiler not support statically linked libs from default folders\"" "$LINENO" 5

                        if test "x$static_linking_support" = "xno"; then
                                LDAP_LIBS=`echo "$LDAP_LIBS"|sed "s|-lldap|$_ldap_dir_lib/libldap.a|g"|sed "s|-llber|$_ldap_dir_lib/liblber.a|g"`
                        fi

                        # without SSL and SASL
                        if test "x$static_linking_support" = "xno"; then
                                TRY_LDAP_LIBS="$LDAP_LIBS -lpthread"
                        else
                                TRY_LDAP_LIBS="${static_linking_support}static $LDAP_LIBS ${static_linking_support}dynamic -lpthread"
                        fi

_save_ldap_cppflags="$CPPFLAGS"
_save_ldap_cflags="$CFLAGS"
_save_ldap_ldflags="$LDFLAGS"
_save_ldap_libs="$LIBS"

LIBS="$LIBS $TRY_LDAP_LIBS"
LDFLAGS="$LDFLAGS $LDAP_LDFLAGS"
CPPFLAGS="$CPPFLAGS $LDAP_CPPFLAGS"
CFLAGS="$CFLAGS "
ldap_link="no"

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <stdio.h>
#include <ldap.h>
#include <lber.h>
#include <ldap_schema.h>

int
main (void)
{

printf("%p,%p", ldap_initialize, ldap_str2attributetype);
printf("%p", ber_free);
return 0;

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :

ldap_link="yes"

fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

CPPFLAGS="$_save_ldap_cppflags"
CFLAGS="$_save_ldap_cflags"
LDFLAGS="$_save_ldap_ldflags"
LIBS="$_save_ldap_libs"
unset _save_ldap_cppflags
unset _save_ldap_cflags
unset _save_ldap_ldflags
unset _save_ldap_libs
if test "x$ldap_link" = "xyes"
then :

                                LDAP_LIBS=$TRY_LDAP_LIBS
                                { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: without SSL" >&5
printf "%s\n" "without SSL" >&6; }

fi

                        # without SSL
                        if test "x$ldap_link" = "xno"; then
                                if test "x$static_linking_support" = "xno"; then
                                        TRY_LDAP_LIBS="$LDAP_LIBS -lpthread -lsasl2"
                                else
                                        TRY_LDAP_LIBS="${static_linking_support}static $LDAP_LIBS ${static_linking_support}dynamic -lpthread -lsasl2"
                                fi

_save_ldap_cppflags="$CPPFLAGS"
_save_ldap_cflags="$CFLAGS"
_save_ldap_ldflags="$LDFLAGS"
_save_ldap_libs="$LIBS"

LIBS="$LIBS $TRY_LDAP_LIBS"
LDFLAGS="$LDFLAGS $LDAP_LDFLAGS"
CPPFLAGS="$CPPFLAGS $LDAP_CPPFLAGS"
CFLAGS="$CFLAGS "
ldap_link="no"

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
