                                        LDAP_LIBS=$TRY_LDAP_LIBS
                                        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: with system OpenSSL linking" >&5
printf "%s\n" "with system OpenSSL linking" >&6; }

