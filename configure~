SHELL
am__quote'
ac_subst_files=''
ac_user_opts='
enable_option_checking
enable_silent_rules
enable_dependency_tracking
enable_largefile
enable_static
enable_static_libs
enable_server
enable_proxy
enable_agent
enable_agent2
