      if test "x$static_linking" = "xno"; then
        as_fn_error $? "static linking is not possible on this system" "$LINENO" 5
      fi
    ;;
  no) ;;
  *) as_fn_error $? "bad value ${enableval} for --enable-static" "$LINENO" 5 ;;
esac
fi


