if test ${ac_cv_path_PG_CONFIG+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  case $PG_CONFIG in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_PG_CONFIG="$PG_CONFIG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
