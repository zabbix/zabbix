		{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for -D_HPUX_API_LEVEL=20040821 compilation flag" >&5
printf %s "checking for -D_HPUX_API_LEVEL=20040821 compilation flag... " >&6; }
	saved_CFLAGS="$CFLAGS"
	CFLAGS="-D_HPUX_API_LEVEL=20040821"
