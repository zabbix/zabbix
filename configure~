then :
  withval=$with_zlib;
			test "x$withval" = "xyes" && withval=/usr
			ZLIB_CFLAGS="-I$withval/include"
			ZLIB_LDFLAGS="-L$withval/lib"
			_zlib_dir_set="yes"


fi



