printf %s "checking for Zabbix server/proxy database selection... " >&6; }
	if test "x$have_db" = "xno"; then
		{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: error" >&5
printf "%s\n" "error" >&6; }
