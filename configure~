    "src/libs/zbxstr/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxstr/Makefile" ;;
    "src/libs/zbxvmware/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxvmware/Makefile" ;;
    "src/libs/zbxsysinfo/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/Makefile" ;;
    "src/libs/zbxsysinfo/agent/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/agent/Makefile" ;;
    "src/libs/zbxsysinfo/aix/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/aix/Makefile" ;;
    "src/libs/zbxsysinfo/alias/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/alias/Makefile" ;;
    "src/libs/zbxsysinfo/common/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/common/Makefile" ;;
    "src/libs/zbxsysinfo/freebsd/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/freebsd/Makefile" ;;
    "src/libs/zbxsysinfo/hpux/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/hpux/Makefile" ;;
    "src/libs/zbxsysinfo/linux/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/linux/Makefile" ;;
    "src/libs/zbxsysinfo/netbsd/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/netbsd/Makefile" ;;
    "src/libs/zbxsysinfo/openbsd/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/openbsd/Makefile" ;;
    "src/libs/zbxsysinfo/osx/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/osx/Makefile" ;;
    "src/libs/zbxsysinfo/simple/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/simple/Makefile" ;;
    "src/libs/zbxsysinfo/solaris/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/solaris/Makefile" ;;
    "src/libs/zbxsysinfo/unknown/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxsysinfo/unknown/Makefile" ;;
    "src/libs/zbxstats/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxstats/Makefile" ;;
    "src/libs/zbxtagfilter/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtagfilter/Makefile" ;;
    "src/libs/zbxtasks/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtasks/Makefile" ;;
    "src/libs/zbxhttppoller/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxhttppoller/Makefile" ;;
    "src/libs/zbxthreads/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxthreads/Makefile" ;;
    "src/libs/zbxtime/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtime/Makefile" ;;
    "src/libs/zbxtimekeeper/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtimekeeper/Makefile" ;;
    "src/libs/zbxtrapper/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtrapper/Makefile" ;;
    "src/libs/zbxtrends/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxtrends/Makefile" ;;
    "src/libs/zbxvariant/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxvariant/Makefile" ;;
    "src/libs/zbxvault/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxvault/Makefile" ;;
    "src/libs/zbxversion/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxversion/Makefile" ;;
    "src/libs/zbxxml/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxxml/Makefile" ;;
    "src/libs/zbxalerter/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxalerter/Makefile" ;;
    "src/libs/zbxpinger/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxpinger/Makefile" ;;
    "src/libs/zbxescalations/Makefile") CONFIG_FILES="$CONFIG_FILES src/libs/zbxescalations/Makefile" ;;
    "src/zabbix_agent/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/Makefile" ;;
    "src/zabbix_agent/logfiles/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/logfiles/Makefile" ;;
    "src/zabbix_agent/active_checks/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/active_checks/Makefile" ;;
    "src/zabbix_agent/agent_conf/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/agent_conf/Makefile" ;;
    "src/zabbix_agent/listener/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/listener/Makefile" ;;
    "src/zabbix_agent/metrics/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_agent/metrics/Makefile" ;;
    "src/zabbix_get/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_get/Makefile" ;;
    "src/zabbix_java/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_java/Makefile" ;;
    "src/zabbix_js/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_js/Makefile" ;;
    "src/zabbix_proxy/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_proxy/Makefile" ;;
    "src/zabbix_proxy/autoreg/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_proxy/autoreg/Makefile" ;;
    "src/zabbix_proxy/cachehistory/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_proxy/cachehistory/Makefile" ;;
    "src/zabbix_proxy/datasender/Makefile") CONFIG_FILES="$CONFIG_FILES src/zabbix_proxy/datasender/Makefile" ;;
