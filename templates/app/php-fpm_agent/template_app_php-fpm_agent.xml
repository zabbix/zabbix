                     <priority>HIGH</priority>
                     <manual_close>YES</manual_close>
                     <dependencies>
                        <dependency>
                           <name>PHP-FPM: Process is not running</name>
                           <expression>{Template App PHP-FPM by Zabbix agent:proc.num["{$PHP_FPM.PROCESS_NAME}"].last()}=0</expression>
                        </dependency>
                     </dependencies>
                  </trigger>
               </triggers>
            </item>
