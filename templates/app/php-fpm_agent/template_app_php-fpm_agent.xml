                     <error_handler_params>0</error_handler_params>
                  </step>
               </preprocessing>
               <master_item>
                  <key>web.page.get["{$PHP_FPM.HOST}","{$PHP_FPM.PING.PAGE}","{$PHP_FPM.PORT}"]</key>
               </master_item>
               <triggers>
                  <trigger>
                     <expression>{last()}=0 or {nodata(3m)}=1</expression>
