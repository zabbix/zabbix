                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <parameters>
                               <parameter>var a = value.match(/server accepts handled requests\s+([0-9]+) ([0-9]+) ([0-9]+)/)
if (a) {
    return a[1]-a[2]
}</parameter>
                            </parameters>
                        </step>
                        <step>
