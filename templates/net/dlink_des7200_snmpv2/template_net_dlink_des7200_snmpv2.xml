         </graph_items>
      </graph>
   </graphs>
   <value_maps>
      <value_map>
         <name>MY-SYSTEM-MIB::mySystemFanIsNormal</name>
         <mappings>
            <mapping>
               <value>1</value>
               <newvalue>noexist</newvalue>
            </mapping>
            <mapping>
               <value>2</value>
               <newvalue>existnopower</newvalue>
