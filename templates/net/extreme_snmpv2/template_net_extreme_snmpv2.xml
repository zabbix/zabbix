            <macro>
               <macro>{$PSU_CRIT_STATUS}</macro>
               <value>3</value>
            </macro>
            <macro>
               <macro>{$TEMP_CRIT_LOW}</macro>
               <value>5</value>
            </macro>
            <macro>
               <macro>{$TEMP_CRIT_STATUS}</macro>
               <value>1</value>
            </macro>
            <macro>
               <macro>{$TEMP_CRIT}</macro>
               <value>65</value>
            </macro>
            <macro>
               <macro>{$TEMP_WARN}</macro>
               <value>55</value>
            </macro>
         </macros>
      </template>
   </templates>
